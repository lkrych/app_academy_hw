<h1>Create New Link</h1>
<form  action="<%= links_url %>" method="post">
  <%= auth_token %>
  <label for="title">Title: </label>
  <input id="title" type="text" name="link[title]" value="<%=@link.title %>">

  <label for="url">URL: </label>
  <input id="url" type="text" name="link[url]" value="<%=@link.url %>">

  <input type="submit" name="" value="Create New Link">

</form>
