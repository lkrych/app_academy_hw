<form class="" action="<%= action_url %>" method="POST">

  <%= auth_token %>
  <label for="email">Email: </label>
  <input id="email" type="text" name="user[email]" placeholder="Email">
  <br>

  <label for="password">Password: </label>
  <input id="password" type="password" name="user[password]">
  <br>

  <input type="submit" value="<%= action_url == session_url ? "Log in" : "Sign Up"%>">

</form>
